{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "scripts/background.js"
  },  
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/duck.png",
      "32": "/images/duck.png",
      "64": "/images/duck.png"
    }
  },
  "icons": {
    "16": "/images/duck.png",
    "32": "/images/duck.png",
    "64": "/images/duck.png"
  },
"options_page": "options.html"
}
